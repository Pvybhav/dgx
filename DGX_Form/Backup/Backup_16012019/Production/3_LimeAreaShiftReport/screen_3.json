{
  "$jason": {
    "head": {
      "title": "Screen-3",
      "description": "Lime Area Shift Report",
      "offline": "true",
      "styles": {
        "no_padding": {
          "padding": "0"
        },
        "center_align": {
          "align": "center"
        },
        "black_color": {
          "color": "rgb(0, 0, 0)"
        },
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 162, 224)"
        },
        "table_header_styles": {
          "size": 15,
          "font": "HelveticaHeader",
          "background": "rgb(14, 53, 112)",
          "color": "rgb(255, 255, 255)",
          "border": "rgb(255, 255, 255)",
          "height": "6%"
        },
        "switch_styles": {
          "color": "rgb(0,255,0)",
          "color:disabled": "rgb(255,0,0)",
          "align": "center"
        },
        "switch_legend_styles": {
          "color": "rgb(0,255,0)",
          "color:disabled": "rgb(255,0,0)",
          "height": "4%",
          "width": "7%"
        },
        "table_header_name_styles": {
          "width": "28%",
          "border": "rgb(255, 255, 255)"
        },
        "table_header_value_styles": {
          "width": "8%",
          "border": "rgb(255, 255, 255)"
        },
        "table_row_odd_styles": {
          "background": "rgb(204,206,213)"
        },
        "table_row_even_styles": {
          "background": "rgb(231,232,235)"
        },
        "submit_button_styles": {
          "width": "15%",
          "height": "7%",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,147,215)",
          "corner_radius": "1%"
        },
        "previous_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 255)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "home_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "save_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        }
      },
      "actions": {
        "$load": {
          "type": "$render"
        },
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$render"
          }
        },
        "handleSave": {
          "type": "$global.set",
          "options": {
            "section_id": "form3_page3",
            "description_of_deficiency": "{{$get.description_of_deficiency}}",
            "comments_operator_description": "{{$get.comments_operator_description}}"
          },
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Saved Successfully",
              "type": "warning"
            }
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "Lime Area Shift Report"
            },
            "style": {
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Description of Deficiency",
                      "class": "table_header_styles",
                      "style": {
                        "width": "100%"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "description_of_deficiency",
                      "value": "{{$global.description_of_deficiency || ''}}",
                      "placeholder": "Enter Description of Deficiency",
                      "contentDescription": "descriptionOfDeficiency",
                      "style": {
                        "width": "100%",
                        "height": "30%",
                        "size": "15"
                      }
                    }
                  ],
                  "class": "no_padding"
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Description",
                      "class": "table_header_styles",
                      "style": {
                        "width": "100%"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "comments_operator_description",
                      "value": "{{$global.comments_operator_description || ''}}",
                      "placeholder": "Enter description",
                      "contentDescription": "Enter Description",
                      "style": {
                        "width": "100%",
                        "height": "30%",
                        "size": "15"
                      }
                    }
                  ],
                  "class": "no_padding"
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align",
                  "style": {
                    "padding_top": "2%",
                    "padding_bottom": "2%"
                  },
                  "components": [
                    {
                      "type": "label",
                      "text": "",
                      "style": {
                        "width": "20%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "previous",
                      "text": "Previous",
                      "class": "previous_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://s3.amazonaws.com/enu-digital-ips/release/production/3_LimeAreaShiftReport/screen_2.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "home",
                      "text": "Home",
                      "class": "home_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://s3.amazonaws.com/enu-digital-ips/release/production/home.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "save",
                      "text": "Save",
                      "class": "save_button_styles center_align",
                      "action": {
                        "trigger": "handleSave"
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "class": "black_color",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "submit",
                      "text": "submit",
                      "class": "center_align submit_button_styles",
                      "contentDescription": "submit",
                      "action": {
                        "type": "$global.reset",
                        "options": {
                          "save": "true",
                          "filename": "LimeAreaShiftReport"
                        },
                        "success": {
                          "type": "$global.reset",
                          "options": {
                            "items": [
                              "section_id",
                              "f3_s1_sign",
                              "f3_s1_date",
                              "f3_s1_month",
                              "f3_s1_year",
                              "f3_s1_shift",
                              "f3_s1_operator",
                              "safety_showers_row1_0700",
                              "safety_showers_row1_1100",
                              "safety_showers_row1_1500",
                              "safety_showers_row2_0700",
                              "safety_showers_row2_1100",
                              "safety_showers_row2_1500",
                              "lime_silo_cone_0700",
                              "lime_silo_cone_1100",
                              "lime_silo_cone_1500",
                              "actuator_0700",
                              "actuator_1100",
                              "actuator_1500",
                              "manual_valves_0700",
                              "manual_valves_1100",
                              "manual_valves_1500",
                              "auto_valves_0700",
                              "auto_valves_1100",
                              "auto_valves_1500",
                              "square_piping_0700",
                              "square_piping_1100",
                              "square_piping_1500",
                              "rotary_valves_0700",
                              "rotary_valves_1100",
                              "rotary_valves_1500",
                              "screw_conveyers_0700",
                              "screw_conveyers_1100",
                              "screw_conveyers_1500",
                              "ventary_conveyers_0700",
                              "ventary_conveyers_1100",
                              "ventary_conveyers_1500",
                              "blower_piping_0700",
                              "blower_piping_1100",
                              "blower_piping_1500",
                              "slater_top_floor_row4_0700",
                              "slater_top_floor_row4_1100",
                              "slater_top_floor_row4_1500",
                              "slater_0700",
                              "slater_1100",
                              "slater_1500",
                              "slater_level_row2_0700",
                              "slater_level_row2_1100",
                              "slater_level_row2_1500",
                              "agitator_0700",
                              "agitator_1100",
                              "agitator_1500",
                              "pumps_0700",
                              "pumps_1100",
                              "pumps_1500",
                              "slater_level_row5_0700",
                              "slater_level_row5_1100",
                              "slater_level_row5_1500",
                              "description_of_deficiency",
                              "comments_operator_description"
                            ]
                          },
                          "success": {
                            "type": "$util.toast",
                            "options": {
                              "text": "Data Submitted successfully",
                              "type": "warning"
                            }
                          }
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "class": "black_color",
                      "style": {
                        "width": "5%"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}