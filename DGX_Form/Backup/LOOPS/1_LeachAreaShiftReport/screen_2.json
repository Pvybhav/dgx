      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)",
              "border": "none"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "center_align table_header_styles {{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "class": "table_body_name_styles table_row_height",
                                  "style": {
                                    "height": "{{#? this.height}}"
                                  },
                                  "components": [
                                    {
                                      "type": "label",
                                      "text": "{{this.row_name}}",
                                      "class": "{{this.row_styles}} table_row_height table_fields_size",
                                      "style": {
                                        "height": "{{#? this.height}}"
                                      }
                                    }
                                  ]
                                },
                                {
                                  "type": "vertical",
                                  "class": "table_body_description_styles table_row_height",
                                  "components": [
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "switch",
                                          "name": "{{this.row_description.true_switch_name}}",
                                          "value": "true",
                                          "class": "switch_styles table_body_value_styles {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                          "isClickable": "false"
                                        },
                                        {
                                          "type": "label",
                                          "name": "{{this.row_description.true_label_name}}",
                                          "text": "{{this.row_description.true_label_value}}",
                                          "class": "black_color table_fields_size"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "switch",
                                          "name": "{{this.row_description.false_switch_name}}",
                                          "value": "false",
                                          "class": "switch_styles table_body_value_styles {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                          "isClickable": "false"
                                        },
                                        {
                                          "type": "label",
                                          "name": "{{this.row_description.false_label_name}}",
                                          "text": "{{this.row_description.false_label_value}}",
                                          "class": "black_color table_fields_size"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "vertical",
                                      "class": "center_align",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "class": "table_body_value_styles",
                                          "components": [
                                            {
                                              "type": "label",
                                              "text": "{{label}}",
                                              "class": "label_styles black_color center_align",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        },
                                        {
                                          "type": "horizontal",
                                          "class": "table_body_value_styles",
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "name": "{{name}}",
                                              "value": "{{var index = $index; return getCurrentFieldValue(index, $root.Table_Rows[row_index].row_values[$index].name, $root['$global']) || 'true';}}",
                                              "class": "center_align switch_styles {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                              "contentDescription": "{{contentDescription}}",
                                              "action": {
                                                "trigger": "handleSwitchValues",
                                                "options": {
                                                  "fieldName": "{{name}}"
                                                }
                                              }
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Description.",
                      "class": "table_header_styles description_title_styles"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "description",
                      "class": "description_styles",
                      "value": "{{$global.f1_s2_description || ''}}",
                      "placeholder": "Enter Description",
                      "contentDescription": "description"
                    }
                  ],
                  "class": "no_padding"
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align footer_styles",
                  "components": [
                    {
                      "type": "space",
                      "style": {
                        "width": "{{(100 % (Object.keys(Footer_Buttons).length*20))-5}}%"
                      }
                    },
                    {
                      "type": "horizontal",
                      "class": "footer_buttons_spacing",
                      "components": {
                        "{{#each Footer_Buttons}}": {
                          "type": "button",
                          "name": "{{this.contentDescription}}",
                          "text": "{{this.displayName}}",
                          "class": "button_styles center_align",
                          "contentDescription": "{{this.contentDescription}}",
                          "action": {
                            "trigger": "{{#? this.trigger}}",
                            "options": {
                              "url": "{{#? this.url}}"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}