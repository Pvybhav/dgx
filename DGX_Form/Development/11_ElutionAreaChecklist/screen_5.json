      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "font": "HelveticaHeader-Bold",
              "background": "rgb(146, 208, 80)",
              "color": "rgb(255,255,255)"
