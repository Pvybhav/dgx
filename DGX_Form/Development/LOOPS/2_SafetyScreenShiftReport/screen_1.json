              "clean_trough_build_up_2_screen1": "{{$global.clean_trough_build_up_2_screen1 == null ? true : $global.clean_trough_build_up_2_screen1}}"
            },
            "success": {
              "type": "$render",
              "success": {
                "trigger": "displayToaster",
                "options": {
                  "message": "Page loaded successfully"
                }
              }
            }
          }
        },
        "displayToaster": {
          "type": "$util.toast",
          "options": {
            "text": "{{$jason.message}}",
            "type": "warning"
          }
        },
        "handleSwitchValues": {
          "type": "$global.set",
          "options": {
            "section_id": "form2_page1",
            "{{$jason.fieldName}}": "{{$get[$jason.fieldName]}}",
            "currentFieldValue": "{{$get[$jason.fieldName]}}"
          }
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$render"
          }
        },
        "handleNavigation": {
          "type": "$href",
          "options": {
            "url": "{{#? $jason.url}}",
            "preload": {
              "class": "preload_styles",
              "layers": [
                {
                  "type": "image",
                  "url": "{{loading_image_url}}",
                  "class": "loading_image_styles"
                }
              ]
            }
          }
        },
        "handleSave": {
          "type": "$global.set",
          "options": {
            "section_id": "form2_page1",
            "f2_s1_sign": "{{$get.sign || ''}}",
            "f2_s1_day": "{{$get.day}}",
            "f2_s1_month": "{{$get.month}}",
            "f2_s1_year": "{{$get.year}}",
            "f2_s1_shift": "{{$get.shift}}",
            "f2_s1_operator": "{{$get.operator}}"
          },
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Saved Successfully",
              "type": "warning"
            }
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "font": "HelveticaHeader-Bold",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)",
              "height": "3%"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align",
                  "style": {
                    "padding_left": "1%",
                    "padding_right": "1%"
                  },
                  "components": [
                    {
                      "type": "dropdown",
                      "name": "day",
                      "value": "{{$global.f2_s1_day}}",
                      "contentDescription": "day",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Days}}": {
                          "value": "{{day}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "dropdown",
                      "name": "month",
                      "value": "{{$global.f2_s1_month}}",
                      "contentDescription": "month",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Months}}": {
                          "value": "{{month}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "dropdown",
                      "name": "year",
                      "contentDescription": "year",
                      "value": "{{$global.f2_s1_year}}",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Years}}": {
                          "value": "{{year}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "shift",
                      "value": "{{$global.f2_s1_shift}}",
                      "contentDescription": "shift",
                      "class": "center_align black_color dropdown_shift_style",
                      "options": {
                        "{{#each Shifts}}": {
                          "value": "{{shift}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "operator",
                      "value": "{{$global.f2_s1_operator}}",
                      "contentDescription": "operator",
                      "class": "center_align black_color dropdown_operator_style",
                      "options": {
                        "{{#each Operators}}": {
                          "value": "{{operator}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "textfield",
                      "value": "{{$global.f2_s1_sign || ''}}",
                      "name": "sign",
                      "placeholder": "Sign",
                      "class": "sign_style center_align black_color",
                      "contentDescription": "Sign"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table1_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table1_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table1_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "class": "table1_body_description_styles",
                                  "components": [
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "switch",
                                          "name": "{{this.row_description.true_switch_name}}",
                                          "value": "true",
                                          "class": "switch_styles table1_body_value_styles table1_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                          "isClickable": "false"
                                        },
                                        {
                                          "type": "label",
                                          "name": "{{this.row_description.true_label_name}}",
                                          "text": "{{this.row_description.true_label_value}}",
                                          "class": "black_color table_fields_size table1_row_height"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "switch",
                                          "name": "{{this.row_description.false_switch_name}}",
                                          "value": "false",
                                          "class": "switch_styles table1_body_value_styles table1_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                          "isClickable": "false"
                                        },
                                        {
                                          "type": "label",
                                          "name": "{{this.row_description.false_label_name}}",
                                          "text": "{{this.row_description.false_label_value}}",
                                          "class": "black_color table_fields_size table1_row_height"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "vertical",
                                      "class": "center_align",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "class": "table1_body_value_styles",
                                          "components": [
                                            {
                                              "type": "label",
                                              "text": "{{label}}",
                                              "class": "label_styles black_color center_align",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        },
                                        {
                                          "type": "horizontal",
                                          "class": "table1_body_value_styles",
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "name": "{{name}}",
                                              "value": "{{var index = $index; return getCurrentFieldValue(index, $root.Table1_Rows[row_index].row_values[$index].name, $root['$global']) || 'true';}}",
                                              "class": "center_align switch_styles {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                              "contentDescription": "{{contentDescription}}",
                                              "action": {
                                                "trigger": "handleSwitchValues",
                                                "options": {
                                                  "fieldName": "{{name}}"
                                                }
                                              }
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table2_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table2_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table2_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "horizontal",
                                      "class": "center_align {{styles}}",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "class": "{{#? this.styles}} {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "text": "{{text}}",
                                              "class": "black_color center_align table_fields_size {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              },
                                              "components": [
                                                {
                                                  "type": "horizontal",
                                                  "components": {
                                                    "{{#each components}}": [
                                                      {
                                                        "{{#if type=='label'}}": {
                                                          "type": "label",
                                                          "text": "{{text}}",
                                                          "class": "{{#? styles}}"
                                                        }
                                                      },
                                                      {
                                                        "{{#elseif type=='switch'}}": {
                                                          "type": "switch",
                                                          "value": "{{true}}",
                                                          "class": "{{#? styles}}"
                                                        }
                                                      }
                                                    ]
                                                  }
                                                }
                                              ]
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align footer_styles",
                  "components": [
                    {
                      "type": "space",
                      "style": {
                        "width": "{{(100 % (Object.keys(Footer_Buttons).length*20))-5}}%"
                      }
                    },
                    {
                      "type": "horizontal",
                      "class": "footer_buttons_spacing",
                      "components": {
                        "{{#each Footer_Buttons}}": {
                          "type": "button",
                          "name": "{{this.contentDescription}}",
                          "text": "{{this.displayName}}",
                          "class": "button_styles center_align",
                          "contentDescription": "{{this.contentDescription}}",
                          "action": {
                            "trigger": "{{#? this.trigger}}",
                            "options": {
                              "url": "{{#? this.url}}"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}