      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "align": "center",
              "font": "HelveticaNeue-Bold",
              "size": 20,
              "background": "rgb(146, 208, 80)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}} center_align"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "class": "table_body_name_styles table_row_height",
                                  "style": {
                                    "height": "{{#? this.height}}"
                                  },
                                  "components": [
                                    {
                                      "type": "label",
                                      "text": "{{this.row_name}}",
                                      "class": "{{this.row_styles}} table_row_height",
                                      "style": {
                                        "height": "{{#? this.height}}"
                                      }
                                    }
                                  ]
                                },
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "vertical",
                                          "class": "table_body_label_width",
                                          "components": [
                                            {
                                              "type": "label",
                                              "text": "{{label}}",
                                              "class": "label_styles black_color center_align table_row_height",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        },
                                        {
                                          "type": "vertical",
                                          "class": "table_row_height",
                                          "style": {
                                            "height": "{{#? this.height}}"
                                          },
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "name": "{{name}}",
                                              "value": "{{var index = $index; return getCurrentFieldValue(index, $root.Table_Rows[row_index].row_values[$index].name, $root['$global']) || '';}}",
                                              "class": "center_align black_color table_body_textfield_styles",
                                              "contentDescription": "{{contentDescription}}",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Notifications.",
                      "class": "table_header_styles comments_title_styles"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "notifications",
                      "class": "comments_styles",
                      "value": "{{$global.f11_s4_notifications || ''}}",
                      "placeholder": "Enter Notifications",
                      "contentDescription": "notifications"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Comments.",
                      "class": "table_header_styles comments_title_styles"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "comments",
                      "class": "comments_styles",
                      "value": "{{$global.f11_s4_comments || ''}}",
                      "placeholder": "Enter Comments",
                      "contentDescription": "comments"
                    }
                  ],
                  "class": "no_padding"
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align footer_styles",
                  "components": [
                    {
                      "type": "space",
                      "style": {
                        "width": "{{(100 % (Object.keys(Footer_Buttons).length*20))-5}}%"
                      }
                    },
                    {
                      "type": "horizontal",
                      "class": "footer_buttons_spacing",
                      "components": {
                        "{{#each Footer_Buttons}}": {
                          "type": "button",
                          "name": "{{this.contentDescription}}",
                          "text": "{{this.displayName}}",
                          "class": "button_styles center_align",
                          "contentDescription": "{{this.contentDescription}}",
                          "action": {
                            "trigger": "{{#? this.trigger}}",
                            "options": {
                              "url": "{{#? this.url}}"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}