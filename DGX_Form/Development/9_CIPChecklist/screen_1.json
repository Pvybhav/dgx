                      "type": "dropdown",
                      "name": "month",
                      "value": "{{$global.f9_s1_month}}",
                      "contentDescription": "month",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Months}}": {
                          "value": "{{month}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "dropdown",
                      "name": "year",
                      "contentDescription": "year",
                      "value": "{{$global.f9_s1_year}}",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Years}}": {
                          "value": "{{year}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "shift",
                      "value": "{{$global.f9_s1_shift}}",
                      "contentDescription": "shift",
                      "class": "center_align black_color dropdown_shift_style",
                      "options": {
                        "{{#each Shifts}}": {
                          "value": "{{shift}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "operator",
                      "value": "{{$global.f9_s1_operator}}",
                      "contentDescription": "operator",
                      "class": "center_align black_color dropdown_operator_style",
                      "options": {
                        "{{#each Operators}}": {
                          "value": "{{operator}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "textfield",
                      "value": "{{$global.f9_s1_sign || ''}}",
                      "name": "sign",
                      "placeholder": "Sign",
                      "class": "sign_style center_align black_color",
                      "contentDescription": "Sign"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "class": "table_body_name_styles table_row_height",
                                  "style": {
                                    "height": "{{#? this.height}}"
                                  },
                                  "components": [
                                    {
                                      "type": "label",
                                      "text": "{{this.row_name}}",
                                      "class": "{{this.row_styles}} table_row_height",
                                      "style": {
                                        "height": "{{#? this.height}}"
                                      }
                                    }
                                  ]
                                },
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "vertical",
                                          "class": "table_body_label_width",
                                          "components": [
                                            {
                                              "type": "label",
                                              "text": "{{label}}",
                                              "class": "label_styles black_color center_align table_row_height",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        },
                                        {
                                          "type": "vertical",
                                          "class": "table_row_height",
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "name": "{{name}}",
                                              "value": "{{var index = $index; return getCurrentFieldValue(index, $root.Table_Rows[row_index].row_values[$index].name, $root['$global']) || '';}}",
                                              "class": "center_align black_color table_body_textfield_styles",
                                              "contentDescription": "{{contentDescription}}"
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align footer_styles",
                  "components": [
                    {
                      "type": "space",
                      "style": {
                        "width": "{{(100 % (Object.keys(Footer_Buttons).length*20))-5}}%"
                      }
                    },
                    {
                      "type": "horizontal",
                      "class": "footer_buttons_spacing",
                      "components": {
                        "{{#each Footer_Buttons}}": {
                          "type": "button",
                          "name": "{{this.contentDescription}}",
                          "text": "{{this.displayName}}",
                          "class": "button_styles center_align",
                          "contentDescription": "{{this.contentDescription}}",
                          "action": {
                            "trigger": "{{#? this.trigger}}",
                            "options": {
                              "url": "{{#? this.url}}"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}