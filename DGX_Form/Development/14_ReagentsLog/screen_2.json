{
  "$jason": {
    "head": {
      "title": "Form-12",
      "json_id": "field",
      "description": "Reagent Log Sheet",
      "offline": "true",
      "styles": {
        "center_align": {
          "align": "center"
        },
        "no_padding": {
          "padding": "0"
        },
        "black_color": {
          "color": "rgb(0,0,0)"
        },
        "label_styles": {
          "font": "HelveticaNeue-Bold"
        },
        "button_styles": {
          "width": "40%",
          "height": "50",
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        },
        "table_row_odd_styles": {
          "background": "rgb(204,206,213)"
        },
        "table_row_even_styles": {
          "background": "rgb(231,232,235)"
        },
        "home_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "next_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 255)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "save_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "dropdown_date_style": {
          "color": "rgb(0, 0, 0)",
          "border": "rgb(0, 0, 0)",
          "width": "7%",
          "height": "3%"
        },
        "right_padding": {
          "padding_left": "-2",
          "padding_right": "2"
        },
        "dropdown_shift_style": {
          "color": "rgb(0, 0, 0)",
          "border": "rgb(0, 0, 0)",
          "width": "20%",
          "height": "3%"
        },
        "dropdown_operator_style": {
          "color": "rgb(0, 0, 0)",
          "border": "rgb(0, 0, 0)",
          "width": "20%",
          "height": "3%"
        },
        "sign_style": {
          "height": "4%",
          "width": "20%",
          "size": "15"
        },
        "submit_button_styles": {
          "width": "15%",
          "height": "7%",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,147,215)",
          "corner_radius": "1%"
        }
      },
      "actions": {
        "$load": {
          "type": "$global.set",
          "options": {
            "section_id": "form12_page2"
          },
          "success": {
            "type": "$render",
            "success": {
              "type": "$util.toast",
              "options": {
                "text": "Page loaded successfully",
                "type": "warning"
              }
            }
          }
        },
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$flush",
          "success": {
            "type": "$reload",
            "success": {
              "type": "$util.toast",
              "options": {
                "text": "Page loaded successfully",
                "type": "warning"
              }
            }
          }
        },
        "handleSave": {
          "type": "$global.set",
          "options": {
            "section_id": "form12_page2",
            "comments": "{{$get.comments || ''}}"
          },
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Saved Successfully",
              "type": "warning"
            }
          }
        }
      },
      "data": {
        "redirect_url": "https://s3.amazonaws.com/enu-digital-ips/development/12_ReagentsLog/screen_1.json",
        "Page_Heading": "Reagents Log Sheet"
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}",
              "align": "left"
            },
            "style": {
              "font": "HelveticaHeader-Bold",
              "size": 15,
              "background": "rgb(154,205,50)",
              "color": "rgb(255,255,255)",
              "height": "0%",
              "padding": "0",
              "align": "left"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "label",
                      "text": "Comments(1)",
                      "style": {
                        "background": "rgb(14, 53, 112)",
                        "width": "100%",
                        "height": "8%",
                        "color": "rgb(255,255,255)",
                        "padding": "1",
                        "font": "HelveticaNeue-Bold",
                        "size": 15
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "textarea",
                      "name": "comments",
                      "contentDescription": "one",
                      "value": "{{$global.comments || ''}}",
                      "style": {
                        "background": "rgb(204,206,213)",
                        "width": "100%",
                        "height": "50%",
                        "color": "rgb(0,0,0)",
                        "padding": "1",
                        "font": "HelveticaNeue"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align",
                  "style": {
                    "padding_top": "1%",
                    "padding_bottom": "1%"
                  },
                  "components": [
                    {
                      "type": "space",
                      "style": {
                        "width": "20%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "previous",
                      "text": "Previous",
                      "class": "next_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://s3.amazonaws.com/enu-digital-ips/development/12_ReagentsLog/screen_1.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "home",
                      "text": "Home",
                      "class": "home_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://s3.amazonaws.com/enu-digital-ips/development/home.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "save",
                      "text": "Save",
                      "class": "save_button_styles center_align",
                      "action": {
                        "trigger": "handleSave"
                      }
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "submit",
                      "text": "Submit",
                      "class": "center_align submit_button_styles",
                      "contentDescription": "submit",
                      "action": {
                        "type": "$global.reset",
                        "options": {
                          "save": "true",
                          "filename": "Reagent_log_sheet"
                        },
                        "success": {
                          "type": "$global.reset",
                          "options": {
                            "items": [
                              "section_id",
                              "f12_s1_sign",
                              "f12_s1_day",
                              "f12_s1_month",
                              "f12_s1_year",
                              "f12_s1_shift",
                              "f12_s1_operator",
                              "textfield1",
                              "textfield2",
                              "textfield3",
                              "textfield4",
                              "textfield5",
                              "textfield6",
                              "textfield7",
                              "textfield8",
                              "textfield9",
                              "textfield10",
                              "textfield11",
                              "textfield12",
                              "textfield13",
                              "textfield14",
                              "textfield15",
                              "textfield16",
                              "textfield17",
                              "textfield18",
                              "textfield19",
                              "textfield20",
                              "textfield21",
                              "textfield22",
                              "textfield23",
                              "textfield24",
                              "comments"
                            ]
                          },
                          "success": {
                            "type": "$href",
                            "options": {
                              "url": "{{redirect_url}}"
                            }
                          }
                        }
                      }
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "5%"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}