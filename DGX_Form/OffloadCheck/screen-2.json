                },
                "black_color": {
                    "color": "rgb(0, 0, 0)"
                },
                "title_styles": {
                    "font": "HelveticaHeader",
                    "size": 20,
                    "align": "center",
                    "color": "rgb(255, 255, 255)",
                    "background": "rgb(0, 162, 224)"
                },
                "header_style": {
                    "background": "rgb(14, 53, 112)",
                    "color": "rgb(255, 255, 255)",
                    "height": "8%",
                    "border": "#ffffff",
                    "padding_left": "2%"

                },
                "content_style_odd": {
                    "background": "rgb(204, 216, 213)",
