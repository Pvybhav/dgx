                    "height": "4%",
                    "width": "7%"
                },
                "switch_table_styles": {
                    "color": "rgb(0,255,0)",
                    "color:disabled": "rgb(255,0,0)",
                    "height": "2%",
                    "width": "5%"
                },
                "table1_col_name_styles": {
                    "width": "37%"
                },
                "table1_col_value_styles": {
                    "width": "5%"
                },
                "table2_col_name_styles": {
                    "width": "20%"
                },
                "table2_col_value_styles": {
                    "width": "30%"
                },
                "table_header_name_styles": {
                    "width": "25%",
                    "border": "rgb(255, 255, 255)"
                },
                "table_header_value_styles": {
                    "width": "5%",
                    "border": "rgb(255, 255, 255)"
                },
                "table_row_odd_styles": {
                    "background": "rgb(204,206,213)"
                },
                "table_row_even_styles": {
                    "background": "rgb(231,232,235)"
                },
                "previous_button_styles": {
                    "color": "rgb(255, 255, 255)",
                    "background": "rgb(0, 102, 255)",
                    "width": "15%",
                    "height": "7%",
                    "corner_radius": "1%"
                },
                "home_button_styles": {
                    "color": "rgb(255, 255, 255)",
                    "background": "rgb(51, 102, 153)",
                    "width": "15%",
                    "height": "7%",
                    "corner_radius": "1%"
                },
                "next_button_styles": {
                    "color": "rgb(255, 255, 255)",
                    "background": "rgb(0, 102, 255)",
                    "width": "15%",
                    "height": "7%",
                    "corner_radius": "1%"
                },
                "save_button_styles": {
                    "color": "rgb(255, 255, 255)",
                    "background": "rgb(51, 102, 153)",
                    "width": "15%",
                    "height": "7%",
                    "corner_radius": "1%"
                }
            },
            "data": {
                "Page_Heading": "Reagent Operator - SO2 Offload Checklist"
               

            },
            "actions": {
                "$load": {
                    "type": "$global.set",
                    "options": {}
                },
                "$pull": {
                    "type": "$reload",
                    "success": {
                        "type": "$render"
                    }
                },
                "handleSave": {
                    "type": "$global.set",
                    "options": {
                        "section_id": "Reagent_Operator",
                        "sign": "{{$get.sign || ''}}",
                        "date": "{{$get.date}}",
                        "month": "{{$get.month}}",
                        "year": "{{$get.year}}",
                        "shift": "{{$get.shift}}",
                        "operator": "{{$get.operator}}",
                        "company": "{{$get.company}}",
                        "truck_driver_name": "{{$get.truck_driver_name}}",
                        "BOL": "{{$get.BOL}}",
                        "Safety_concerns": "{{$get.Safety_concerns}}",
                        "warehouse_tech_name": "{{$get.warehouse_tech_name}}"


                    },
                    "success": {
                        "type": "$util.toast",
                        "options": {
                            "text": "Saved Successfully",
                            "type": "warning"
                        }
                    }
                }
            },
            "templates": {
                "body": {
                    "header": {
                        "title": {
                            "type": "label",
                            "text": "{{Page_Heading}}"
                        },
                        "style": {
                            "font": "HelveticaHeader",
                            "size": 20,
                            "background": "rgb(146,208,80)",
                            "color": "rgb(255,255,255)",
                            "height": "3%"
                        }
                    },
                    "sections": [

                        {
                            "type": "vertical",
                            "items": [{
                                    "type": "horizontal",
                                    "style": {
                                        "color": "rgb(255, 255, 255)",
                                        "padding_bottom": "1%"
                                    },
                                    "components": [{
                                            "type": "label",
                                            "text": "Checks",
                                            "style": {
                                                "width": "60%"
                                            },
                                            "class": "header_style"
                                        },
                                        {
                                            "type": "label",
                                            "text": " ",
                                            "style": {
                                                "width": "10%"
                                            },
                                            "class": "header_style"
                                        },
                                        {
                                            "type": "label",
                                            "text": "Comments",
                                            "style": {
                                                "width": "30%"
                                            },
                                            "class": "header_style"
                                        }
                                    ],
                                    "class": "no_padding"
                                },
                                {
                                    "type": "horizontal",
                                    "style": {
                                        "color": "rgb(255, 255, 255)",
                                        "padding_bottom": "1%"
                                    },
                                    "components": [{
                                            "type": "label",
                                            "text": "Install safety perimeter cones",
                                            "style": {
                                                "width": "60%"
                                            },
                                            "class": "content_style_odd"
                                        },
                                        {
                                            
                                                "type": "switch",
                                                "name": "trash_screen1_rollers_2",
                                                "value": "{{$global.trash_screen1_rollers_2}}",
                                                "class": "switch_styles content_style_odd",
                                                "contentDescription": "two",
                                                "action": {
                                                  "type": "$global.set",
                                                  "options": {
                                                    "trash_screen1_rollers_2": "{{$get.trash_screen1_rollers_2}}"
                                                  }
                                                }
                                              

                                        },
                                        {
                                            "type": "textarea",
                                            "value": "{{$global.comment1 || ''}}",
                                            "name": "comment1",
                                            "contentDescription": "comment1",
                                            "style": {
                                                "width": "30%",
                                                "height":"8%"
                                                
                                            },
                                            "class":"content_style_odd_textfield"
                                            
                                        }
                                    ],
                                    "class": "no_padding"
                                },
                                {
                                    "type": "horizontal",
                                    "style": {
                                        "color": "rgb(255, 255, 255)",
                                        "padding_bottom": "1%"
                                    },
                                    "components": [{
                                            "type": "label",
                                            "text": "Ensure Truck wheels are chocked",
                                            "style": {
                                                "width": "60%"
                                            },
                                            "class": "content_style_even"
                                        },
                                        {
                                            
                                                "type": "switch",
                                                "name": "trash_screen1_rollers_2",
                                                "value": "{{$global.trash_screen1_rollers_2}}",
                                                "class": "switch_styles content_style_even",
                                                "contentDescription": "two",
                                                "action": {
                                                  "type": "$global.set",
                                                  "options": {
                                                    "trash_screen1_rollers_2": "{{$get.trash_screen1_rollers_2}}"
                                                  }
                                                }
                                              

                                        },
                                        {
                                            "type": "textarea",
                                            "name": "TS_strainer_upstream_screen1",
                                            "value": "{{$global.TS_strainer_upstream_screen1 || ''}}",
                                            "class": "center_align content_style_even_textfield",
                                            "contentDescription": "sixteen",
                                            "style": {
                                              "placeholder_color": "#cecece",
                                              "font": "HelveticaNeue",
                                              "border": "#ffffff",
                                              "width": "30%",
                                              "height": "8%"
                                              
                                              
                                            }
            
                                          }
                                    ],
                                    "class": "no_padding"
                                }

                            ],
                            "class": "no_padding"
                        },
                        
                        {
                            "type": "horizontal",
                            "items": [{
                                "type": "horizontal",
                                "class": "no_padding center_align",
                                "style": {
                                    "padding_top": "1%",
                                    "padding_bottom": "1%"
                                },
                                "components": [{
                                        "type": "label",
                                        "text": "",
                                        "class": "black_color",
                                        "style": {
                                            "width": "40%"
                                        }
                                    },
                                    {
                                        "type": "button",
                                        "name": "home",
                                        "text": "Home",
                                        "class": "home_button_styles center_align",
                                        "action": {
                                            "type": "$href",
                                            "options": {
                                                "url": "https://s3.amazonaws.com/enu-digital-ips/dg-forms-static/home.json",
                                                "preload": {
                                                    "style": {
                                                        "background": "#ff0000"
                                                    },
                                                    "layers": [{
                                                        "type": "image",
                                                        "url": "file://infinit.gif",
                                                        "style": {
                                                            "top": "50%-25",
                                                            "left": "50%-25",
                                                            "width": "50",
                                                            "height": "50"
                                                        }
                                                    }]
                                                }
                                            }
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "text": "",
                                        "class": "black_color",
                                        "style": {
                                            "width": "5%"
                                        }
                                    },
                                    {
                                        "type": "button",
                                        "name": "next",
                                        "text": "Next",
                                        "class": "next_button_styles center_align",
                                        "action": {
                                            "type": "$href",
                                            "options": {
                                                "url": "https://pvybhav.github.io/DGX_Form/OffloadCheck/screen-3.json",
                                                "preload": {
                                                    "style": {
                                                        "background": "#ff0000"
                                                    },
                                                    "layers": [{
                                                        "type": "image",
                                                        "url": "file://infinit.gif",
                                                        "style": {
                                                            "top": "50%-25",
                                                            "left": "50%-25",
                                                            "width": "50",
                                                            "height": "50"
                                                        }
                                                    }]
                                                }
                                            }
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "text": "",
                                        "class": "black_color",
                                        "style": {
                                            "width": "5%"
                                        }
                                    },
                                    {
                                        "type": "button",
                                        "name": "save",
                                        "text": "Save",
                                        "class": "save_button_styles center_align",
                                        "action": {
                                            "trigger": "handleSave"
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "text": "",
                                        "class": "black_color",
                                        "style": {
                                            "width": "5%"
                                        }
                                    }
                                ]
                            }]
                        }
                    ]
                }
            }
        }
    }
}