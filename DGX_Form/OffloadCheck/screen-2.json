                    "height": "8%",
                    "border": "#ffffff",
                    "padding_left": "2%"
                },
                "content_style_even_textfield": {
                    "background": "rgb(231, 232, 235)",
                    "color": "rgb(0, 0, 0)",
                    "border": "#ffffff"
                },
                "dropdown_date_style": {
                    "color": "rgb(0, 0, 0)",
                    "border": "rgb(0, 0, 0)",
                    "width": "7%",
                    "height": "3%"
                },
                "dropdown_shift_style": {
                    "color": "rgb(0, 0, 0)",
                    "border": "rgb(0, 0, 0)",
                    "width": "20%",
                    "height": "3%"
                },
                "dropdown_operator_style": {
                    "color": "rgb(0, 0, 0)",
                    "border": "rgb(0, 0, 0)",
                    "width": "20%",
                    "height": "3%"
                },
                "sign_style": {
                    "height": "4%",
                    "width": "20%",
                    "size": "15"

                },
                "header_2_styles": {
                    "size": 13,
                    "width": "24%",
                    "height": "8%",
                    "color": "rgb(0, 0, 0)",
                    "border": "rgb(0, 0, 0)"
                },
                "header_2_label": {
                    "size": 13,
                    "width": "19%",
                    "color": "rgb(0,162,224)",
                    "align": "right"
                },
                "header_2_labels_styles": {
                    "size": 13,
                    "width": "25%",
                    "height": "7%",
                    "color": "rgb(0,162,224)"
                },
                "table_header_styles": {
                    "size": 15,
                    "font": "HelveticaHeader",
                    "background": "rgb(14, 53, 112)",
                    "color": "rgb(255, 255, 255)",
                    "border": "rgb(255, 255, 255)",
                    "height": "6%"
                },
                "roller_header_style": {
                    "size": 15,
                    "font": "HelveticaHeader",
                    "background": "rgb(14, 53, 112)",
                    "color": "rgb(255, 255, 255)",
                    "border": "rgb(255, 255, 255)",
                    "height": "12%"
                },
                "switch_styles": {
                    "color": "rgb(0,255,0)",
