                      "name": "month",
                      "value": "{{$global.form17_month}}",
                      "contentDescription": "month",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Months}}": {
                          "value": "{{month}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "dropdown",
                      "name": "year",
                      "contentDescription": "year",
                      "value": "{{$global.form17_year}}",
                      "class": "dropdown_day_style center_align",
                      "options": {
                        "{{#each Years}}": {
                          "value": "{{year}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "shift",
                      "value": "{{$global.form17_shift}}",
                      "contentDescription": "shift",
                      "class": "center_align black_color dropdown_shift_style",
                      "options": {
                        "{{#each Shifts}}": {
                          "value": "{{shift}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "dropdown",
                      "name": "operator",
                      "value": "{{$global.form17_operator}}",
                      "contentDescription": "operator",
                      "class": "center_align black_color dropdown_operator_style",
                      "options": {
                        "{{#each Operators}}": {
                          "value": "{{operator}}",
                          "defaultSelected": "{{defaultSelected}}"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "▼",
                      "class": "right_padding"
                    },
                    {
                      "type": "space",
                      "style": {
                        "width": "2%"
                      }
                    },
                    {
                      "type": "textfield",
                      "value": "{{$global.form17_sign || ''}}",
                      "name": "sign",
                      "placeholder": "Sign",
                      "class": "sign_style center_align black_color",
                      "contentDescription": "Sign"
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table_Headers1}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each Table_Headers2}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "vertical",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "vertical",
                      "components": {
                        "{{#each Table1_Rows}}": {
                          "{{#let}}": [
                            {
                              "row_index": "{{$index}}"
                            },
                            {
                              "type": "horizontal",
                              "class": "center_align table_row_height {{ row_index % 2 == 0 ? 'table_even_row_styles' : 'table_odd_row_styles'}}",
                              "style": {
                                "height": "{{#? this.height}}"
                              },
                              "components": [
                                {
                                  "type": "horizontal",
                                  "components": {
                                    "{{#each row_values}}": {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "vertical",
                                          "class": "table_body_label_width",
                                          "components": [
                                            {
                                              "type": "label",
                                              "text": "{{#? label}}",
                                              "class": "label_styles black_color center_align table_row_height",
                                              "style": {
                                                "height": "{{#? this.height}}"
                                              }
                                            }
                                          ]
                                        },
                                        {
                                          "type": "vertical",
                                          "class": "table_row_height",
                                          "components": [
                                            {
                                              "type": "{{type}}",
                                              "name": "{{name}}",
                                              "text": "{{#? text}}",
                                              "value": "{{var index = $index; return getCurrentFieldValue(index, $root.Table1_Rows[row_index].row_values[$index].name, $root['$global']) || '';}}",
                                              "class": "center_align black_color table_body_textfield_styles {{type=='label'? 'label_styles table_row_height body_row_label_styles': ''}}",
                                              "contentDescription": "{{contentDescription}}"
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ],
          "layers": [
            {
              "type": "image",
              "url": "{{home_icon}}",
              "contentDescription": "home",
              "class": "layer_styles",
              "style": {
                "left": "20%"
              },
              "action": {
                "trigger": "handleNavigation",
                "options": {
                  "url": "{{redirect_url}}"
                }
              }
            },
            {
              "type": "button",
              "text": "1",
              "class": "layer_styles template_button_styles active_template_button_styles no_padding",
              "contentDescription": "page-one",
              "style": {
                "left": "30%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen1"
                }
              }
            },
            {
              "type": "button",
              "text": "2",
              "class": "layer_styles template_button_styles no_padding",
              "contentDescription": "page-two",
              "style": {
                "left": "40%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen2"
                }
              }
            },
            {
              "type": "image",
              "url": "{{next_icon}}",
              "class": "layer_styles",
              "contentDescription": "next",
              "style": {
                "left": "50%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen2"
                }
              }
            },
            {
              "type": "image",
              "url": "{{save_icon}}",
              "class": "layer_styles",
              "contentDescription": "save",
              "style": {
                "left": "60%"
              },
              "action": {
                "trigger": "handleSave_screen1"
              }
            }
          ]
        },
        "screen2": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}} (2/2)",
              "align": "left"
            },
            "style": {
              "font": "HelveticaHeader-Bold",
              "size": 15,
              "background": "rgb(154,205,50)",
              "color": "rgb(255,255,255)",
              "height": "0%",
              "padding": "0",
              "align": "left"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "class": "no_padding",
                      "components": [
                        {
                          "type": "label",
                          "text": "Comments.",
                          "class": "table_header_styles",
                          "style": {
                            "width": "100%"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "textarea",
                      "value": "{{$global.form17_comments || ''}}",
                      "name": "comments",
                      "placeholder": "Enter comments",
                      "contentDescription": "comments",
                      "class": "comments_styles"
                    }
                  ],
                  "class": "no_padding"
                }
              ]
            }
          ],
          "layers": [
            {
              "type": "image",
              "url": "{{home_icon}}",
              "contentDescription": "home",
              "class": "layer_styles",
              "style": {
                "left": "20%"
              },
              "action": {
                "trigger": "handleNavigation",
                "options": {
                  "url": "{{redirect_url}}"
                }
              }
            },
            {
              "type": "image",
              "url": "{{previous_icon}}",
              "class": "layer_styles",
              "contentDescription": "previous",
              "style": {
                "left": "30%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen1"
                }
              }
            },
            {
              "type": "button",
              "text": "1",
              "class": "layer_styles template_button_styles",
              "contentDescription": "page-one",
              "style": {
                "left": "40%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen1"
                }
              }
            },
            {
              "type": "button",
              "text": "2",
              "contentDescription": "page-two",
              "class": "layer_styles template_button_styles active_template_button_styles",
              "style": {
                "left": "50%"
              },
              "action": {
                "trigger": "handleTemplateChange",
                "options": {
                  "templateName": "screen2"
                }
              }
            },
            {
              "type": "image",
              "url": "{{save_icon}}",
              "class": "layer_styles",
              "contentDescription": "save",
              "style": {
                "left": "60%"
              },
              "action": {
                "trigger": "handleSave_screen2"
              }
            },
            {
              "type": "image",
              "url": "{{submit_icon}}",
              "contentDescription": "submit",
              "class": "layer_styles",
              "style": {
                "left": "70%"
              },
              "action": {
                "trigger": "handleSubmit"
              }
            }
          ]
        }
      }
    }
  }
