{
  "$jason": {
    "head": {
      "title": "Dynamic Data Rendering from URL",
      "description": "Fetching data from remote json and displaying values",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "http://web.jasonette.com/items/1045.json"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "sections": [
            {
              "items": {
                "{{#each $jason.members}}": {
                  "type": "label",
                  "name": "{{id}}",
                  "value": "{{value}}"
                }
              }
            }
          ]
        }
      }
    }
  }
}