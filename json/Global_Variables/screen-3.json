{
  "$jason": {
    "head": {
      "title": "TextField",
      "styles": {
        "padded": {
          "padding": "20",
          "font": "HelveticaNeue-Bold",
          "size": "20"
        }
      }
    },
    "body": {
      "style": {
        "background": "#b6dce6"
      },
      "sections": [
        {
          "header": {
            "type": "label",
            "text": "Screen-3",
            "style": {
              "background": "#f5f5f5",
              "align": "center"
            }
          },
          "items": [
            {
              "type": "horizontal",
              "style": {
                "background": "#ffffff",
                "align": "center"
              },
              "components": [
                {
                  "type": "textfield",
                  "style": {
                    "border_color": "#000000"
                  },
                  "placeholder": "Enter your name",
                  "name": "name"
                }
              ]
            }
          ]
        }
      ]
    }
  }
}