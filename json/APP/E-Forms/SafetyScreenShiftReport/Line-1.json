{
  "$jason": {
    "head": {
      "title": "Line-1",
      "description": "Line-1 by using Loops",
      "offline": "true",
      "styles": {
        "no_padding": {
          "padding": "0"
        },
        "center_align": {
          "align": "center"
        },
        "black": {
          "color": "rgb(0,0,0)"
        },
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,162,224)"
        },
        "header_2_styles": {
          "size": 13,
          "width": "20%",
          "color": "rgb(0,162,224)",
          "border": "rgb(0,0,0)"
        },
        "header_2_label": {
          "size": 13,
          "width": "20%",
          "color": "rgb(0,162,224)"
        },
        "table_header_styles": {
          "size": 15,
          "font": "HelveticaHeader",
          "background": "rgb(14, 53, 112)",
          "color": "rgb(255, 255, 255)",
          "border": "rgb(255, 255, 255)",
          "height": "6%"
        },
        "button_styles": {
          "width": "40%",
          "height": 50,
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        },
        "switch_styles": {
          "color": "#00ff00",
          "color:disabled": "#ff0000"
        },
        "switch_legend_styles": {
          "color": "#00ff00",
          "color:disabled": "#ff0000",
          "height": "4%",
          "width": "7%"
        },
        "col_name_styles": {
          "width": "15%"
        },
        "col_legend_styles": {
          "width": "25%"
        },
        "col_value_styles": {
          "width": "6%"
        },
        "table_row_odd_styles": {
          "background": "rgb(204,206,213)"
        },
        "table_row_even_styles": {
          "background": "rgb(231,232,235)"
        },
        "next_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 255)",
          "width": "30%",
          "height": "10%",
          "corner_radius": "1%"
        },
        "home_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "30%",
          "height": "10%",
          "corner_radius": "1%"
        }
      },
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://pvybhav.github.io/json/APP/data/SafetyScreenShiftReport/Line-1.json"
          },
          "success": {
            "type": "$render"
          }
        },
        "$show": {
          "type": "$network.request",
          "options": {
            "url": "https://pvybhav.github.io/json/APP/data/SafetyScreenShiftReport/Line-1.json"
          },
          "success": {
            "type": "$render"
          }
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{$jason.Page_Heading}}"
            },
            "style": {
              "align": "center",
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "label",
                  "text": "11/12/2018",
                  "class": "header_2_styles center_align black"
                },
                {
                  "type": "label",
                  "text": "Day Shift",
                  "class": "header_2_styles center_align black"
                },
                {
                  "type": "label",
                  "text": "* Operator",
                  "class": "header_2_styles center_align black"
                },
                {
                  "type": "label",
                  "text": "Signed",
                  "class": "header_2_label center_align"
                },
                {
                  "type": "label",
                  "text": "Jean Pronovost",
                  "class": "header_2_styles center_align black"
                }
              ],
              "class": "no_padding"
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each $jason.Table_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding center_align",
                  "components": [
                    {
                      "type": "vertical",
                      "components": [
                        {
                          "type": "vertical",
                          "components": {
                            "{{#each $jason.Table_Body}}": {
                              "type": "horizontal",
                              "components": [
                                {
                                  "type": "horizontal",
                                  "components": [
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "components": [
                                            {
                                              "type": "horizontal",
                                              "components": [
                                                {
                                                  "type": "vertical",
                                                  "components": [
                                                    {
                                                      "type": "horizontal",
                                                      "components": {
                                                        "{{#each this[0].rowValues}}": {
                                                          "type": [
                                                            {
                                                              "{{#if type=='label'}}": "label"
                                                            },
                                                            {
                                                              "{{#else type!='label'}}": "horizontal"
                                                            }
                                                          ],
                                                          "components": [
                                                            [
                                                              {
                                                                "{{#if key=='legend'}}": {
                                                                  "type": "vertical",
                                                                  "components": [
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "switch",
                                                                          "name": "{{key}}",
                                                                          "value": "true",
                                                                          "class": "switch_legend_styles"
                                                                        },
                                                                        {
                                                                          "type": "label",
                                                                          "name": "{{key}}",
                                                                          "text": "= {{checked}}"
                                                                        }
                                                                      ]
                                                                    },
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "switch",
                                                                          "name": "{{key}}",
                                                                          "value": "false",
                                                                          "class": "switch_legend_styles"
                                                                        },
                                                                        {
                                                                          "type": "label",
                                                                          "name": "{{key}}",
                                                                          "text": "= {{unchecked}}"
                                                                        }
                                                                      ]
                                                                    }
                                                                  ]
                                                                }
                                                              },
                                                              {
                                                                "{{#elseif key!='legend'}}": {
                                                                  "type": "vertical",
                                                                  "components": [
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "label",
                                                                          "text": "{{contentDescription || ' '}}",
                                                                          "name": "{{key}}",
                                                                          "class": "center_align"
                                                                        }
                                                                      ]
                                                                    },
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "switch",
                                                                          "value": "{{value}}",
                                                                          "name": "{{key}}",
                                                                          "class": "{{styles}} center_align"
                                                                        }
                                                                      ]
                                                                    }
                                                                  ]
                                                                }
                                                              }
                                                            ]
                                                          ],
                                                          "text": "{{value}}",
                                                          "name": "{{key}}",
                                                          "class": "{{styles}}",
                                                          "style": [
                                                            {
                                                              "{{#if type=='label'}}": {
                                                                "padding": "1%"
                                                              }
                                                            }
                                                          ]
                                                        }
                                                      },
                                                      "style": {
                                                        "height": "15%"
                                                      }
                                                    }
                                                  ]
                                                }
                                              ]
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ],
                                  "class": "{{this[0].styles}} no_padding"
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "style": {
                "padding": "2%"
              },
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align",
                  "style": {
                    "padding": "2%"
                  },
                  "components": [
                    {
                      "type": "label",
                      "text": "",
                      "style": {
                        "width": "35%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "home",
                      "text": "Home",
                      "class": "home_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/Home.json"
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "next",
                      "text": "Next",
                      "class": "next_button_styles",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/SafetyScreenShiftReport/Line-2.json"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}