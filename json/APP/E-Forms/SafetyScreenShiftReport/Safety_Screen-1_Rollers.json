                                              "components": [
                                                {
                                                  "type": "vertical",
                                                  "components": [
                                                    {
                                                      "type": "horizontal",
                                                      "components": {
                                                        "{{#each this[0].rowValues}}": {
                                                          "type": [
                                                            {
                                                              "{{#if key=='legend'}}": "horizontal"
                                                            },
                                                            {
                                                              "{{#else key!='legend'}}": "{{type}}"
                                                            }
                                                          ],
                                                          "text": "{{value}}",
                                                          "name": "{{key}}",
                                                          "value": "{{value}}",
                                                          "class": "{{styles}} center_align",
                                                          "style": [
                                                            {
                                                              "{{#if key=='legend'}}": {
                                                                "width": "{{width}}"
