{
  "$jason": {
    "head": {
      "title": "Line-1",
      "description": "Line-2 by using Loops",
      "offline": "true",
      "styles": {
        "no_padding": {
          "padding": "0"
        },
        "no_border": {
          "border": "none"
        },
        "center_align": {
          "align": "center"
        },
        "black_color": {
          "color": "rgb(0, 0, 0)"
        },
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 162, 224)"
        },
        "header_2_styles": {
          "size": 13,
          "width": "24%",
          "height": "8%",
          "color": "rgb(0, 0, 0)",
          "border": "rgb(0, 0, 0)"
        },
        "header_2_label": {
          "size": 13,
          "width": "19%",
          "color": "rgb(0,162,224)",
          "align": "right"
        },
        "header_2_labels_styles": {
          "size": 13,
          "width": "25%",
          "height": "7%",
          "color": "rgb(0,162,224)"
        },
        "table_header_styles": {
          "size": 15,
          "font": "HelveticaHeader",
          "background": "rgb(14, 53, 112)",
          "color": "rgb(255, 255, 255)",
          "border": "rgb(255, 255, 255)",
          "height": "6%"
        },
        "switch_styles": {
          "color": "rgb(0,255,0)",
          "color:disabled": "rgb(255,0,0)",
          "align": "center"
        },
        "switch_legend_styles": {
          "color": "rgb(0,255,0)",
          "color:disabled": "rgb(255,0,0)",
          "height": "4%",
          "width": "7%"
        },
        "table_header_name_styles": {
          "width": "37%",
          "border": "rgb(255, 255, 255)"
        },
        "table_header_value_styles": {
          "width": "7%",
          "border": "rgb(255, 255, 255)"
        },
        "table_row_odd_styles": {
          "background": "rgb(204,206,213)"
        },
        "table_row_even_styles": {
          "background": "rgb(231,232,235)"
        },
        "previous_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 255)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "home_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(51, 102, 153)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        },
        "next_button_styles": {
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 255)",
          "width": "15%",
          "height": "7%",
          "corner_radius": "1%"
        }
      },
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://raw.githubusercontent.com/Pvybhav/pvybhav.github.io/master/json/APP/data/SafetyScreenShiftReport/Safety_Screen-1_Rollers.json",
            "preload": {
              "style": {
                "background": "#ff0000"
              },
              "layers": [
                {
                  "type": "image",
                  "url": "file://infinit.gif",
                  "style": {
                    "top": "50%-25",
                    "left": "50%-25",
                    "width": "50",
                    "height": "50"
                  }
                }
              ]
            }
          },
          "success": {
            "type": "$render"
          }
        },
        "$show": {
          "type": "$network.request",
          "options": {
            "url": "https://raw.githubusercontent.com/Pvybhav/pvybhav.github.io/master/json/APP/data/SafetyScreenShiftReport/Safety_Screen-1_Rollers.json",
            "preload": {
              "style": {
                "background": "#ff0000"
              },
              "layers": [
                {
                  "type": "image",
                  "url": "file://infinit.gif",
                  "style": {
                    "top": "50%-25",
                    "left": "50%-25",
                    "width": "50",
                    "height": "50"
                  }
                }
              ]
            }
          },
          "success": {
            "type": "$render"
          }
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
