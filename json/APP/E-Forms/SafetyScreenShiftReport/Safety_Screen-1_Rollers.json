                                                                      "components": [
                                                                        {
                                                                          "type": "label",
                                                                          "text": "{{contentDescriptionText || ' '}}",
                                                                          "class": "center_align black_color",
                                                                          "style": {
                                                                            "width": "9%"
                                                                          }
                                                                        },
                                                                        {
                                                                          "type": "switch",
                                                                          "name": "{{key}}",
                                                                          "value": "{{value}}",
                                                                          "class": "switch_styles",
                                                                          "contentDescription": "{{contentDescription || ' '}}",
                                                                          "style": {
                                                                            "width": "8%"
                                                                          }
                                                                        },
                                                                        {
                                                                          "type": "label",
                                                                          "text": "",
                                                                          "class": "center_align black_color",
                                                                          "style": {
                                                                            "width": "8%"
                                                                          }
                                                                        }
                                                                      ]
                                                                    }
                                                                  ]
