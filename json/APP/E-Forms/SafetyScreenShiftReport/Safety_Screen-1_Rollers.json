            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each $jason.Table1_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding center_align",
                  "components": [
                    {
                      "type": "vertical",
                      "components": [
                        {
                          "type": "vertical",
                          "components": {
                            "{{#each $jason.Table1_Body}}": {
                              "type": "horizontal",
                              "components": [
                                {
                                  "type": "horizontal",
                                  "components": [
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "components": [
                                            {
                                              "type": "horizontal",
                                              "components": [
                                                {
                                                  "type": "vertical",
                                                  "components": [
                                                    {
                                                      "type": "horizontal",
                                                      "components": {
                                                        "{{#each this[0].rowValues}}": {
                                                          "type": [
                                                            {
                                                              "{{#if key=='legend'}}": "horizontal"
                                                            },
                                                            {
                                                              "{{#else key!='legend'}}": "horizontal"
                                                            }
                                                          ],
                                                          "text": "{{value}}",
                                                          "name": "{{key}}",
                                                          "value": "{{value}}",
                                                          "contentDescription": "{{contentDescription}}",
                                                          "class": "{{styles}} center_align",
                                                          "style": [
                                                            {
                                                              "{{#if key=='legend'}}": {
                                                                "height": "10%",
                                                                "width": "{{width}}"
                                                              }
                                                            },
                                                            {
                                                              "{{#else key!='legend'}}": {
                                                                "height": "10%",
                                                                "width": "7%"
                                                              }
                                                            }
                                                          ],
                                                          "components": [
                                                            [
                                                              {
                                                                "{{#if key=='legend'}}": {
                                                                  "type": "vertical",
                                                                  "components": [
                                                                    {
                                                                      "type": "horizontal",
                                                                      "class": "{{styles}}",
                                                                      "components": [
                                                                        {
                                                                          "type": "horizontal",
                                                                          "components": [
                                                                            {
                                                                              "type": "switch",
                                                                              "name": "{{key}}",
                                                                              "value": "{{value}}",
                                                                              "class": "switch_styles"
                                                                            },
                                                                            {
                                                                              "type": "label",
                                                                              "name": "{{key}}",
                                                                              "text": "= {{checked}}",
                                                                              "class": "black_color"
                                                                            }
                                                                          ]
                                                                        },
                                                                        {
                                                                          "type": "horizontal",
                                                                          "components": [
                                                                            {
                                                                              "type": "switch",
                                                                              "name": "{{key}}",
                                                                              "value": "false",
                                                                              "class": "switch_styles"
                                                                            },
                                                                            {
                                                                              "type": "label",
                                                                              "name": "{{key}}",
                                                                              "text": "= {{unchecked}}",
                                                                              "class": "black_color"
                                                                            }
                                                                          ]
                                                                        }
                                                                      ]
                                                                    }
                                                                  ]
                                                                }
                                                              },
                                                              {
                                                                "{{#elseif key!='legend'}}": {
                                                                  "type": "vertical",
                                                                  "components": [
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "label",
                                                                          "text": "{{contentDescriptionText || ' '}}",
                                                                          "name": "{{key}}",
                                                                          "class": "center_align black_color"
                                                                        }
                                                                      ],
                                                                      "style": {
                                                                        "height": "4%"
                                                                      }
                                                                    },
                                                                    {
                                                                      "type": "horizontal",
                                                                      "components": [
                                                                        {
                                                                          "type": "switch",
                                                                          "value": "{{value}}",
                                                                          "name": "{{key}}",
                                                                          "class": "{{styles}} center_align",
                                                                          "contentDescription": "{{contentDescription || ' '}}"
                                                                        }
                                                                      ],
                                                                      "style": {
                                                                        "height": "6%"
                                                                      }
                                                                    }
                                                                  ]
                                                                }
                                                              }
                                                            ]
                                                          ]
                                                        }
                                                      }
                                                    }
                                                  ]
                                                }
                                              ]
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ],
                                  "class": "{{this[0].styles}} no_padding"
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
