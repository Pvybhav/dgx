                                                              }
                                                            ]
                                                          ]
                                                        }
                                                      },
                                                      "style": {
                                                        "height": "7%"
                                                      }
                                                    }
                                                  ]
                                                }
                                              ]
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ],
                                  "class": "{{this[0].styles}} no_padding"
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding center_align",
                  "style": {
                    "padding_top": "2%",
                    "padding_bottom": "2%",
                    "border": "none"
                  },
                  "components": [
                    {
                      "type": "label",
                      "text": "",
                      "class": "black_color",
                      "style": {
                        "width": "60%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "home",
                      "text": "Home",
                      "class": "home_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://raw.githubusercontent.com/Pvybhav/pvybhav.github.io/master/json/APP/Home.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "label",
                      "text": "",
                      "class": "black_color",
                      "style": {
                        "width": "5%"
                      }
                    },
                    {
                      "type": "button",
                      "name": "next",
                      "text": "Next",
                      "class": "next_button_styles center_align",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://raw.githubusercontent.com/Pvybhav/pvybhav.github.io/master/json/APP/E-Forms/SafetyScreenShiftReport/Safety_Screen-2_Rollers.json",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://infinit.gif",
                                "style": {
                                  "top": "50%-25",
                                  "left": "50%-25",
                                  "width": "50",
                                  "height": "50"
                                }
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "label",
