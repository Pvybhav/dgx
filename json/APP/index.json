                      {
                        "type": "label",
                        "text": "Agitator Shaft"
                      },
                      {
                        "type": "label",
                        "text": "Filter Bypass Indicator"
                      },
                      {
                        "type": "label",
                        "text": "Gearbox Oil Sight Glass"
                      },
                      {
                        "type": "label",
                        "text": "Gearbox Vibration"
                      },
                      {
                        "type": "label",
