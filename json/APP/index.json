{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "align": "center",
          "font": "HelveticaHeader",
          "size": 20,
          "background": "rgb(0,162,224)",
          "color": "rgb(255,255,255)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "DETOUR GOLD CONNECTED WORKER",
              "class": "title_styles"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "components": [
                    {
                      "type": "button",
                      "text": "Line-1",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/Line-1.json"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "components": [
                    {
                      "type": "button",
                      "text": "Line-2",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/Line-2.json"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}