{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "align": "center",
          "font": "HelveticaHeader",
          "size": 20,
          "background": "rgb(0,162,224)",
          "color": "rgb(255,255,255)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        }
      },
      "templates": {
        "body": {
          "sections": [
            {
              "type": "vertical",
              "items": [
                {
                  "type": "button",
                  "text": "Go to Screen-1",
                  "style": {
                    "align": "center",
                    "width": "100%",
                    "height": 50,
                    "color": "#ffffff",
                    "background": "#0000ff"
                  },
                  "action": {
                    "type": "$href",
                    "options": {
                      "url": "https://pvybhav.github.io/json/APP/Line-1.json"
                    }
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}