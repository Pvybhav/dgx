{
  "$jason": {
    "head": {
      "title": "Switch",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "left_align_switch_styles": {
          "color": "rgb(0, 255, 0)",
          "color:disabled": "rgb(255,0,0)"
        },
        "center_align_switch_styles": {
          "color": "rgb(0, 255, 0)",
          "color:disabled": "rgb(255,0,0)"
        },
        "right_align_switch_styles": {
          "color": "rgb(0, 255, 0)",
          "color:disabled": "rgb(255,0,0)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        },
        "banner": {
          "type": "$util.banner",
          "options": {
            "title": "{{$get[$jason.item].toString()}}",
            "description": "{{$jason.item}}"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": "Switch",
            "style": {
              "background": "rgb(0, 0, 204)",
              "color": "rgb(255, 255, 255)"
            }
          },
          "sections": [
            {
              "type": "vertical",
              "components": [
                {
                  "type": "horizontal",
                  "components": [
                    {
                      "type":"label",
                      "text":"123"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}