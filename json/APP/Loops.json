{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,162,224)"
        },
        "no_padding": {
          "padding": "0"
        },
        "center_align": {
          "align": "center"
        },
        "table_header_styles": {
          "size": 18,
          "font": "HelveticaHeader",
          "border": "rgb(255,255,255)",
          "background": "rgb(14,53,112)",
          "color": "rgb(255,255,255)"
        },
        "button_styles": {
          "width": "40%",
          "height": 50,
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        },
        "switch_styles": {
          "color": "#00ff00",
          "color:disabled": "#ff0000",
          "width": "6%"
        },
        "col_name_styles": {
          "width": "20%"
        },
        "col_value_styles": {
          "width": "6%"
        },
        "table_row_odd_styles": {
          "background": "rgb(204,206,213)",
          "color": "rgb(0,0,0)"
        },
        "table_row_even_styles": {
          "background": "rgb(231,232,235)",
          "color": "rgb(0,0,0)"
        }
      },
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://pvybhav.github.io/json/APP/data/Data_Loops.json"
          },
          "success": {
            "type": "$render"
          }
        },
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{$jason.Page_Heading}}"
            },
            "style": {
              "align": "center",
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding",
                  "components": [
                    {
                      "type": "horizontal",
                      "components": {
                        "{{#each $jason.Table_Headers}}": {
                          "type": "label",
                          "text": "{{value}}",
                          "class": "{{styles}}"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "vertical",
                  "class": "no_padding center_align",
                  "components": [
                    {
                      "type": "vertical",
                      "components": [
                        {
                          "type": "vertical",
                          "components": {
                            "{{#each $jason.Table_Body}}": {
                              "type": "horizontal",
                              "components": [
                                {
                                  "type": "horizontal",
                                  "components": [
                                    {
                                      "type": "horizontal",
                                      "components": [
                                        {
                                          "type": "horizontal",
                                          "components": [
                                            {
                                              "type": "horizontal",
                                              "components": [
                                                {
                                                  "type": "vertical",
                                                  "components": [
                                                    {
                                                      "type": "horizontal",
                                                      "components": {
                                                        "{{#each this[0].rowValues}}": {
                                                          "type": "label",
                                                          "name": "{{key}}",
                                                          "text": "{{contentDescription || ' '}}",
                                                          "class": "center_align no_padding {{col_type}}_styles",
                                                          "style": {
                                                            "color": "rgb(14,53,112)"
                                                          }
                                                        }
                                                      },
                                                      "class": "{{this[0].styles}}"
                                                    },
                                                    {
                                                      "type": "horizontal",
                                                      "components": {
                                                        "{{#each this[0].rowValues}}": {
                                                          "type": "{{type}}",
                                                          "text": "{{value}}",
                                                          "name": "{{key}}",
                                                          "value": "{{value}}",
                                                          "contentDescription": "{{contentDescription}}",
                                                          "class": "{{styles}} center_align"
                                                        }
                                                      }
                                                    }
                                                  ]
                                                }
                                              ]
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ],
                                  "class": "{{this[0].styles}} no_padding"
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}