              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": {
                    "{{#each Table_Headers}}": {
                      "type": "label",
                      "text": "{{col_name}}",
                      "class": "table_header center_align {{col_type}}"
                    }
                  }
                }
              ]
            },
            {
              "type": "vertical",
              "items": {
                "{{#each Table_Body}}": {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "label",
                      "text": "{{this[0].fieldName}}"
                    },
                    {
                      "{{#each this[0].values}}": {
                        "type": "label",
                        "text": "{{value}}",
                        "class": "center_align {{col_type}}"
                      },
                      "class": "no_padding"
                    }
                  ]
                }
              }
            }
          ]
        }
