      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "align": "center",
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": {
                    "{{#each Table_Headers}}": {
                      "type": "label",
                      "text": "{{col_name}}",
                      "class": "table_header center_align {{col_type}}"
                    }
                  }
                }
              ]
            },
            {
              "type": "vertical",
              "items": {
                "{{#each Table_Body}}": {
                  "type": "horizontal",
                  "components": [
                    {
                      "{{#each this[0].rowKeys}}": {
                        "type": "label",
                        "text": "{{value}}",
                        "class": "center_align {{col_type}}"
                      }
                    },
                    {
                      "{{#each this[0].rowValues}}": {
                        "type": "switch",
                        "name": "{{key}}",
                        "value": "{{value}}",
                        "class": "center_align {{col_type}} switch_styles"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "vertical",
              "items": {
                "{{#each Table_Body}}": {
                  "type": "horizontal",
                  "components": {
                    "{{#each this[0].rowValues}}": {
                      "type": "switch",
                      "name": "{{key}}",
                      "value": "{{value}}",
                      "class": "center_align {{col_type}} switch_styles"
                    }
                  }
                }
              }
            }
          ]
