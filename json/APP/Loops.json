{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,162,224)"
        },
        "no_padding": {
          "padding": "0"
        },
        "center_align": {
          "align": "center"
        },
        "table_header": {
          "size": 18,
          "font": "HelveticaHeader",
          "border": "rgb(255,255,255)",
          "background": "rgb(14,53,112)",
          "color": "rgb(255,255,255)"
        },
        "button_styles": {
          "width": "40%",
          "height": 50,
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        }
      },
      "data": {
        "Page_Heading": "Loops",
        "Table_Headers": [
          {
            "text": "Line-1",
            "width": "20%"
          },
          {
            "text": "",
            "width": "20%"
          },
          {
            "text": "101",
            "width": "6%"
          },
          {
            "text": "102",
            "width": "6%"
          },
          {
            "text": "103",
            "width": "6%"
          },
          {
            "text": "104",
            "width": "6%"
          },
          {
            "text": "105",
            "width": "6%"
          },
          {
            "text": "111",
            "width": "6%"
          },
          {
            "text": "112",
            "width": "6%"
          },
          {
            "text": "113",
            "width": "6%"
          },
          {
            "text": "114",
            "width": "6%"
          },
          {
            "text": "115",
            "width": "6%"
          }
        ],
        "Table_Rows": []
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "label",
              "text": "{{Page_Heading}}"
            },
            "style": {
              "align": "center",
              "font": "HelveticaHeader",
              "size": 20,
              "background": "rgb(0,162,224)",
              "color": "rgb(255,255,255)"
            }
          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": {
                    "{{#each Table_Headers}}": {
                      "type": "label",
                      "text": "{{text}}",
                      "class": "table_header center_align",
                      "style": {
                        "width": "{{width}}"
                      }
                    }
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}