          },
          "sections": [
            {
              "type": "horizontal",
              "items": [
                {
                  "type": "horizontal",
                  "class": "no_padding",
                  "components": {
                    "{{#each Table_Headers}}": {
                      "type": "label",
                      "text": "{{col_name}}",
                      "class": "table_header center_align {{col_type}}"
                    }
                  }
                }
              ]
            },
            {
              "type": "vertical",
              "items": {
                "{{#each Table_Body}}": {
                  "type": "horizontal",
                  "components": [
                    {
                      "type":"label",
                      "text": "{{fieldName}}",
                      "class": "center_align"
                    },
                    {
                      "{{#each this[0].values}}": {
                        "type": "label",
                        "text": "{{value}}",
                        "class": "table_header center_align",
                        "style": {
                          "width": "6%"
                        }
                      }
                    }
                  ],
                  "class": "no_padding"
                }
              }
            }
