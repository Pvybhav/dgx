{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,162,224)"
        },
        "center_align": {
          "align": "center"
        },
        "left_align": {
          "align": "left"
        },
        "eform_button_styles": {
          "width": "80%",
          "height": "15%",
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        },
        "eform_voicetag_styles": {
          "width": "20%",
          "height": "15%",
          "color": "rgb(0, 102, 204)",
          "background": "rgb(102, 255, 204)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        }
      },
      "templates": {
        "body": {
          "sections": [
            {
              "type": "vertical",
              "class": "center_align",
              "items": [
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form-1",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Leach Area Shift Checklist",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form1",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/LeachAreaShiftReport/Line-1.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form-2",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Safety Screen Shift Report",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form2",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/SafetyScreenShiftReport/Line-1.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form-3",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Lime Area Shift Report",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form3",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/LimeAreaShiftReport/LimeAreaShiftReport-1.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "label",
                                "text": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form-4",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Carbon Input to PI",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form4",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/CarbonCountInputForm/CarbonCountInputForm.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "label",
                                "text": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Switch",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Switch",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Switch",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/switch.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "label",
                                "text": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Tutorial",
                      "class": "center_align eform_voicetag_styles"
                    },
                    {
                      "type": "button",
                      "text": "Tutorial",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Tutorial",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://jasonette.github.io/Jasonpedia/hello.json",
                          "loading": "true",
                          "preload": {
                            "style": {
                              "background": "#ff0000"
                            },
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}