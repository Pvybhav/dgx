{
  "$jason": {
    "head": {
      "title": "APP",
      "description": "A jasonette app",
      "offline": "true",
      "styles": {
        "title_styles": {
          "font": "HelveticaHeader",
          "size": 20,
          "align": "center",
          "color": "rgb(255,255,255)",
          "background": "rgb(0,162,224)"
        },
        "center_align": {
          "align": "center"
        },
        "left_align": {
          "align": "left"
        },
        "eform_button_styles": {
          "width": "90%",
          "height": "15%",
          "space": "15",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)"
        },
        "eform_voicetag_styles": {
          "width": "10%",
          "height": "15%",
          "color": "rgb(0, 102, 204)",
          "background": "rgb(102, 255, 204)"
        }
      },
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        }
      },
      "templates": {
        "body": {
          "sections": [
            {
              "type": "vertical",
              "class": "center_align",
              "items": [
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form1",
                      "class": "center_align eform_voicetag_styles",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/LeachAreaShiftReport/Line-1.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "button",
                      "text": "  Leach Area Shift Checklist",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form1",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/LeachAreaShiftReport/Line-1.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "horizontal",
                  "class": "center_align",
                  "components": [
                    {
                      "type": "button",
                      "text": "Form2",
                      "class": "center_align eform_voicetag_styles",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/SafetyScreenShiftReport/Safety_Screen-1_Rollers.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    },
                    {
                      "type": "button",
                      "text": "  Safety Screen Shift Report",
                      "class": "left_align eform_button_styles",
                      "contentDescription": "Form2",
                      "action": {
                        "type": "$href",
                        "options": {
                          "url": "https://pvybhav.github.io/json/APP/E-Forms/SafetyScreenShiftReport/Safety_Screen-1_Rollers.json",
                          "loading": "true",
                          "preload": {
                            "layers": [
                              {
                                "type": "image",
                                "url": "file://loading.jpeg"
                              }
                            ]
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}