{
  "$jason": {
    "head": {
      "title": "Templates",
      "description": "Templates Practise",
      "offline": "true",
      "actions": {
        "$show": {
          "type": "$render"
        },
        "$pull": {
          "type": "$reload"
        },
        "reload": {
          "type": "$reload"
        }
      },
      "data": {
        "members": [
          {
            "name": "Homer"
          },
          {
            "name": "Marge"
          },
          {
            "name": "Lisa"
          },
          {
            "name": "Bart"
          },
          {
            "name": "Maggie"
          }
        ]
      },
      "templates": {
        "body": {
          "sections": [
            {
              "items": [
                {
                  "type": "image",
                  "url": "{{$jason.image}}"
                },
                {
                  "type": "label",
                  "text": "{{$jason.username}}"
                }
              ]
            },
            {
              "items": [
                {
                  "type": "label",
                  "url": "Full JSON string"
                },
                {
                  "type": "label",
                  "text": "{{JSON.stringify($jason)}}"
                }
              ]
            },
            {
              "items": [
                {
                  "type": "label",
                  "url": "Reversed Fullname"
                },
                {
                  "type": "label",
                  "text": "{{var sorted_posts = $jason.posts.sort(function(a,b){ return new Date(b.created_at) - new Date(a.created_at); }); return sorted_posts[0];}}"
                }
              ]
            },
            {
              "items": {
                "{{#each members}}": {
                  "type": "label",
                  "text": "{{name}}"
                }
              }
            }
          ]
        }
      }
    }
  }
}