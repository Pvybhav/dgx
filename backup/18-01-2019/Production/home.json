{
  "$jason": {
    "head": {
      "title": "DG Forms",
      "description": "Detour Gold E-Forms",
      "offline": "true",
      "styles": {
        "center_align": {
          "align": "center"
        },
        "left_align": {
          "align": "left"
        },
        "eform_button_styles": {
          "width": "90%",
          "height": "10%",
          "color": "rgb(255, 255, 255)",
          "background": "rgb(0, 102, 204)",
          "size": 20
        },
        "eform_voicetag_styles": {
          "width": "10%",
          "height": "10%",
          "font": "HelveticaNeue-Bold",
          "color": "rgb(0, 0, 0)",
          "background": "rgb(102, 255, 204)",
          "size": 20
        }
      },
      "actions": {
        "$load": {
          "type": "$render",
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Page loaded successfully",
              "type": "warning"
            }
          }
        },
        "$show": {
          "type": "$render",
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Page loaded successfully",
              "type": "warning"
            }
          }
        },
        "$pull": {
          "type": "$reload",
          "success": {
            "type": "$util.toast",
            "options": {
              "text": "Page loaded successfully",
              "type": "warning"
            }
          }
        }
